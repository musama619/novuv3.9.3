    "sinon": "^9.2.4",
    "ts-node": "~10.9.1",
    "typescript": "5.6.2",
    "liquidjs": "^10.20.1"
  },
  "peerDependencies": {
    "@nestjs/common": "10.4.18",
    "@nestjs/platform-express": "10.4.18",
    "@nestjs/swagger": "7.4.0",
    "@novu/dal": "workspace:*"
