    "@novu/shared": "workspace:*",
    "@novu/notifications": "workspace:*",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "^4.1.0",
    "mongoose": "^7.8.7",
    "rxjs": "7.8.1",
    "shortid": "^2.2.16",
    "stripe": "^11.18.0",
    "stripe-event-types": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
